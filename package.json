{
  "name": "repair-lab",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "prisma": {
    "seed": " ts-node --esm prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && npm run format",
    "lint:strict": "eslint src",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test": "npm run docker:up && prisma db push && vitest --no-threads run",
    "coverage": "prisma migrate deploy && vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force && prisma db push",
    "prisma:studio": "prisma studio",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.21.3",
    "@headlessui/react": "^1.7.15",
    "@prisma/client": "^4.15.0",
    "@types/chai": "^4.3.5",
    "axios": "^1.4.0",
    "next": "13.4.6",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-icons": "^4.9.0",
    "react-query": "^3.39.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.3.2",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.32.2",
    "autoprefixer": "10.4.14",
    "cross-env": "^7.0.3",
    "deep-equal-in-any-order": "^2.0.6",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jsdom": "^22.1.0",
    "next-test-api-route-handler": "^3.1.8",
    "postcss": "8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.15.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.32.2"
  }
}
