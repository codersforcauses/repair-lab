{
  "name": "repair-lab",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "src/email"
  ],
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && npm run format",
    "lint:strict": "eslint src",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "test": "npm run docker:up && prisma db push && vitest --no-threads --reporter verbose run",
    "coverage": "vitest run --coverage --no-threads",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset --force && prisma db push",
    "prisma:studio": "prisma studio",
    "postinstall": "npm run prisma:generate",
    "sesmock": "docker run -d -p 8005:8005 pensk/aws-ses-v2-local"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.449.0",
    "@aws-sdk/client-ses": "^3.449.0",
    "@aws-sdk/s3-presigned-post": "^3.456.0",
    "@aws-sdk/s3-request-presigner": "^3.473.0",
    "@clerk/nextjs": "^4.27.1",
    "@clerk/types": "^3.58.0",
    "@headlessui/react": "^1.7.17",
    "@prisma/client": "^5.6.0",
    "@react-email/render": "^0.0.9",
    "@tanstack/react-query": "^5.17.10",
    "@tanstack/react-query-devtools": "^5.17.5",
    "@tanstack/react-table": "^8.11.0",
    "axios": "^1.4.0",
    "clsx": "^2.1.0",
    "next": "^14.0.2",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "tailwind-merge": "^2.2.0",
    "tailwind-scrollbar": "^3.0.5",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@hookform/devtools": "^4.3.1",
    "@hookform/resolvers": "^3.3.2",
    "@testing-library/react": "^14.1.0",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "deep-equal-in-any-order": "^2.0.6",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "^22.1.0",
    "next-test-api-route-handler": "^3.1.10",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
